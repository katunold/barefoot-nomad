{
  "name": "barefoot-nomad-backend",
  "version": "1.0.0",
  "description": "A platform to make company global travel and accommodation easy and convenient for the strong workforce of savvy members of staff, by leveraging the modern web.",
  "main": "index.js",
  "scripts": {
    "start:dev": "npm run migrations && cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "test": "NODE_ENV=test nyc mocha --require @babel/register --exit",
    "coverage": "npm run test && nyc report --reporter=text-lcov | coveralls",
    "migrations": "sequelize db:migrate",
    "migrations:undo:all": "sequelize db:migrate:undo:all",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "start": "npm run migrations:undo:all && cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint .",
    "reset-db": "sequelize db:drop && sequelize db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katunold/barefoot-nomad.git"
  },
  "author": "Arnold Katumba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katunold/barefoot-nomad/issues"
  },
  "homepage": "https://github.com/andela/blackninjas-backend#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.4.3",
    "@sendgrid/mail": "^6.4.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-validator": "^6.3.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-plus-token": "^2.1.0",
    "pd": "^0.8.1",
    "pg": "^7.14.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.1",
    "nyc": "^14.1.1",
    "prettier": "1.19.1",
    "sinon": "^7.5.0"
  }
}
