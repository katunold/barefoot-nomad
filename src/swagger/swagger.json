  "parameters": {
    "register": {
      "in": "body",
      "name": "Resend",
      "description": "User sign-up",
      "schema": {
        "type": "object",
        "required": [
          "firstName",
          "lastName",
          "email",
          "password"
        ],
        "properties": {
          "firstName": {
            "type": "string",
            "default": false
          },
          "lastName": {
            "type": "string",
            "default": false
          },
          "email": {
            "type": "string",
            "default": false
          },
          "password": {
            "type": "string",
            "format": "password",
            "minLength": 8
          }
        },
        "example": {
          "firstName": "test",
          "lastName": "client",
          "email": "testclient@mail.com",
          "password": "1qaz2wsx"
        }
      }
    },
    "resend": {
      "in": "body",
      "name": "Resend",
      "description": "Resend verification email",
      "schema": {
        "type": "object",
        "required": [
          "email"
        ],
        "properties": {
          "email": {
            "type": "string",
            "default": false
          }
        },
        "example": {
          "email": "testclient@mail.com"
        }
      }
    },
    "login": {
      "in": "body",
      "name": "Login",
      "description": "Login user",
      "schema": {
        "type": "object",
        "required": [
          "email",
          "password"
        ],
        "properties": {
          "email": {
            "type": "string",
            "default": false
          },
          "password": {
            "type": "string",
            "default": false
          }
        },
        "example": {
          "email": "testclient@mail.com",
          "password": "1qaz2wsx"
        }
      }
    },
    "reset-password": {
      "in": "body",
      "name": "Reset Password",
      "description": "Request for a password reset",
      "schema": {
        "type": "object",
        "required": [
          "email"
        ],
        "properties": {
          "email": {
            "type": "string",
            "default": false
          }
        },
        "example": {
          "email": "testclient@mail.com"
        }
      }
    },
    "new-password": {
      "in": "body",
      "name": "Set new password",
      "description": "Set a new password",
      "schema": {
        "type": "object",
        "required": [
          "password"
        ],
        "properties": {
          "password": {
            "type": "string",
            "default": false
          }
        },
        "example": {
          "password": "1qaz2wsx"
        }
      }
    },
    "google": {
      "in": "body",
      "name": "Sign-up/login with google",
      "description": "User social auth using google",
      "schema": {
        "type": "object",
        "required": [
          "access_token"
        ],
        "properties": {
          "access_token": {
            "type": "string"
          }
        },
        "example": {
          "access_token": "yfuetglqyuiewlfu;38oeiwruf;yiqerulgfyuertekwuygfqegwfgkqt4t4t6i78i96ol"
        }
      }
    },
    "facebook": {
      "in": "body",
      "name": "Sign-up/login with facebook",
      "description": "User social auth using facebook",
      "schema": {
        "type": "object",
        "required": [
          "access_token"
        ],
        "properties": {
          "access_token": {
            "type": "string"
          }
        },
        "example": {
          "access_token": "yfuetglqyuiewlfu;38oeiwruf;yiqerulgfyuertekwuygfqegwfgkqt4t4t6i78i96ol"
        }
      }
    },
    "profile": {
      "in": "body",
      "name": "Profile",
      "description": "User should able to update fields of the profile",
      "schema": {
        "type": "object",
        "required": [
          "profilePic",
          "firstName",
          "lastName",
          "email",
          "gender",
          "birthDate",
          "residence",
          "role",
          "department",
          "lineManager",
          "preferredLanguage",
          "preferredCurrency"
        ],
        "properties": {
          "profilePic": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "gender": {
            "type": "string"
          },
          "birthDate": {
            "type": "string"
          },
          "residence": {
            "type": "string"
          },
          "role": {
            "type": "string"
          },
          "department": {
            "type": "string"
          },
          "lineManager": {
            "type": "string"
          },
          "preferredLanguage": {
            "type": "string"
          },
          "preferredCurrency": {
            "type": "string"
          }
        },
        "example": {
          "gender": "m",
          "birthDate": "1994-10-25",
          "residence": "Kampala",
          "role": "Software Engineer",
          "department": "Partner Engineering",
          "lineManager": "Magero",
          "preferredLanguage": "English",
          "preferredCurrency": "usd"
        }
      }
    },
    "trip": {
      "in": "body",
      "name": "one way trip",
      "description": "User can submit a request for a one way trip here",
      "schema": {
        "type": "object",
        "required": [
          "departureDate",
          "returnDate",
          "travelReason",
          "accommodationId"
        ],
        "properties": {
          "departureDate": {
            "type": "string",
            "default": false
          },
          "returnDate": {
            "type": "string",
            "default": false
          },
          "travelReason": {
            "type": "string",
            "default": false
          },
          "accommodationId": {
            "type": "string",
            "default": false
          }
        },
        "example": {
          "departureDate": "2020-01-04",
          "returnDate": "2020-01-06",
          "travelReason": "Business oriented",
          "accommodationId": "Acadia suits"
        }
      }
    },
    "accommodation": {
      "in": "body",
      "name": "Create accommodation facility",
      "description": "Supplier can add an accommodation facility",
      "schema": {
        "type": "object",
        "required": [
          "facilityName",
          "locationId",
          "numberOfRooms",
          "description",
          "address"
        ],
        "properties": {
          "facilityName": {
            "type": "string",
            "default": false
          },
          "locationId": {
            "type": "integer",
            "default": false
          },
          "numberOfRooms": {
            "type": "integer",
            "default": false
          },
          "description": {
            "type": "string",
            "default": false
          },
          "address": {
            "type": "string",
            "default": false
          }
        },
        "example": {
          "facilityName": "Acadia suites",
          "locationId": 10,
          "numberOfRooms": 50,
          "description": "Heaven away from heaven",
          "address": "Plot 103, Kira road"
        }
      }
    }
